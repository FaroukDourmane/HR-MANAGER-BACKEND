{
  "collectionName": "components_user_jobs",
  "info": {
    "displayName": "Job",
    "description": ""
  },
  "options": {},
  "attributes": {
    "job_title": {
      "type": "string",
      "required": true
    },
    "department": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::department.department"
    },
    "grade": {
      "type": "integer"
    },
    "hire_date": {
      "type": "date",
      "required": true
    },
    "hire_end": {
      "type": "date"
    },
    "employment_type": {
      "type": "enumeration",
      "enum": [
        "full-time",
        "part-time",
        "intern",
        "temporary"
      ],
      "required": true,
      "default": "full-time"
    },
    "shift": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::shift.shift"
    },
    "country": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::country.country"
    }
  }
}
